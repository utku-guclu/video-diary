{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "jsx": "react", // or "react-jsx" for React 17+
    "strict": true,
    "types": [
      "jest",
      "node",
    ],
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./app/*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    "nativewind-env.d.ts"
  ]
}